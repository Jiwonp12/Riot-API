{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" },
    { "source": "/riot-tag", "destination": "/api/riot-tag" },
    { "source": "/riot-puuid", "destination": "/api/riot-puuid" }
  ],
  "functions": {
    "api": {
      "excludeFiles": ["node_modules/**/*", ".git/**/*"],
      "runtime": "nodejs12.x"
    }
  }
}
